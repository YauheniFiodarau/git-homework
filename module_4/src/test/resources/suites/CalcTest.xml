<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="UnitCalcTest">
    <test name="Calculator" parallel="classes">
        <classes>
            <class name="by.fiodarau.CosTest"></class>
            <class name="by.fiodarau.SinTest"></class>
            <class name="by.fiodarau.CtgTest"></class>
            <class name="by.fiodarau.TgTest"></class>
            <class name="by.fiodarau.IsNegativeTest"></class>
            <class name="by.fiodarau.IsPositiveTest"></class>
            <class name="by.fiodarau.DivDoubleTest"></class>
            <class name="by.fiodarau.DivLongTest"></class>
            <class name="by.fiodarau.MultiplyDoubleTest"></class>
            <class name="by.fiodarau.MultiplyLongTest"></class>
            <class name="by.fiodarau.SqrtTest"></class>
            <class name="by.fiodarau.SubDoubleTest"></class>
            <class name="by.fiodarau.SubLongTest"></class>
            <class name="by.fiodarau.SumLongTest"></class>
            <class name="by.fiodarau.SumDoubleTest"></class>
        </classes>
    </test>

    <test name="OnlyTrigonometry">
        <groups>
            <run>
                <include name="Trigonometry"></include>
                <exclude name="Arithmetic"></exclude>
            </run>
        </groups>
        <classes>
            <class name="by.fiodarau.CosTest"></class>
            <class name="by.fiodarau.SinTest"></class>
            <class name="by.fiodarau.CtgTest"></class>
            <class name="by.fiodarau.TgTest"></class>
            <class name="by.fiodarau.IsNegativeTest"></class>
            <class name="by.fiodarau.IsPositiveTest"></class>
            <class name="by.fiodarau.DivDoubleTest"></class>
            <class name="by.fiodarau.DivLongTest"></class>
            <class name="by.fiodarau.MultiplyDoubleTest"></class>
            <class name="by.fiodarau.MultiplyLongTest"></class>
            <class name="by.fiodarau.SqrtTest"></class>
            <class name="by.fiodarau.SubDoubleTest"></class>
            <class name="by.fiodarau.SubLongTest"></class>
            <class name="by.fiodarau.SumLongTest"></class>
            <class name="by.fiodarau.SumDoubleTest"></class>
        </classes>
    </test>
</suite>